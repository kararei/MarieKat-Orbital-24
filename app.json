{
  "expo": {
    "name": "MarieKat",
    "slug": "MarieKat",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "mariekat",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "package": "com.mariekat"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f0458dc8-508a-465b-bb61-5388c1008f81"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/f0458dc8-508a-465b-bb61-5388c1008f81"
    }
  }
}
